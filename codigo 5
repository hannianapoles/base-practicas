#include<iostream>
using namespace std;
int num,num1,num2,num3,suma;
int resta,mult,divide;
int main(){
cout<<"digita el primer numero";
cin>>num;
cout<<"digita el segundo numero";
cin>>num2;
cout<<"digita el tercer numero";
cin<<num3;
suma=num+num1+num2+num3;
resta=num-num1-num2-num3;
mult=num*num1*num2*num3;
divide=num/num1/num2/num3;
cout<<"la suma es"<<suma;
cout<<"la resta es"<<resta;
cout<<"la multiplicacion es"<<mult;
cout<<"la division es"<<divide;
cin.get;
return 0;
